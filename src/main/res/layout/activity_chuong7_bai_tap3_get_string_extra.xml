            android:text="Get String Extra Activity" />

        <TextView
            android:id="@+id/c7_b3_supActivity_txtHoTen"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:textSize="20sp"
            android:text="Ho Ten Khach Hang: " />

        <Button
            android:id="@+id/c7_b3_supActivity_btnBackMain"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Back To Main Activity" />
    </LinearLayout>
